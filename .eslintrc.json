{
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4,
            {
                "ArrayExpression": "first"
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error"
        ],
        "comma-spacing": [
            "error"
        ],
        "space-before-function-paren": [
            "error",
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-before-blocks": [
            "error"
        ],
        "key-spacing": [
            "error"
        ],
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-var": "error",
        "no-console": 0
    }
}
